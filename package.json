{
  "name": "spaces",
  "version": "0.1.0",
  "description": "Agent workspace manager for Claude, Codex, Gemini, and more",
  "main": "electron/dist/main.js",
  "bin": {
    "spaces": "./bin/spaces.js"
  },
  "scripts": {
    "dev": "next dev --port 3457",
    "dev:desktop": "npm run compile && concurrently -n next,electron \"npm run dev\" \"wait-on http://127.0.0.1:3457 && electron .\"",
    "compile": "tsc -p electron/tsconfig.json",
    "build": "ELECTRON=1 next build && npm run compile",
    "pack": "npm run build && electron-builder --dir",
    "dist:linux": "npm run build && electron-builder --linux",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "server:build": "next build",
    "server:start": "next start --port 3457",
    "lint": "eslint",
    "postinstall": "electron-rebuild -f -w better-sqlite3,node-pty 2>/dev/null || true"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "@types/qrcode": "^1.5.6",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "geist": "^1.4.1",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "node-pty": "^1.1.0",
    "otpauth": "^9.5.0",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.5.0",
    "ws": "^8.19.0",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.1.2",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
